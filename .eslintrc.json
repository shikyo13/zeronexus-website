{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-undef": "warn"
    },
    "globals": {
        "bootstrap": "readonly",
        "$": "readonly",
        "jQuery": "readonly"
    }
}